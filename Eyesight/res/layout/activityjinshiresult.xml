<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >

    <include layout="@layout/toplayout" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20.0dip"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.0dip"
                android:layout_marginRight="10.0dip"
                android:background="@drawable/semangceshi" >

                <TextView
                    android:id="@+id/text_lefteye_shuzhi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="5.0dip"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />

                <ProgressBar
                    android:id="@+id/progress_left1"
                    style="?android:progressBarStyleHorizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginRight="10.0dip"
                    android:layout_marginTop="10.0dip"
                    android:layout_toRightOf="@id/text_lefteye_shuzhi"
                    android:max="1500"
                    android:progressDrawable="@drawable/progressbar" />

                <ProgressBar
                    android:id="@+id/progress_left2"
                    style="?android:progressBarStyleHorizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginRight="10.0dip"
                    android:layout_marginTop="10.0dip"
                    android:layout_toRightOf="@id/text_lefteye_shuzhi"
                    android:max="1500"
                    android:progressDrawable="@drawable/progressbaryb" />

                <ProgressBar
                    android:id="@+id/progress_left3"
                    style="?android:progressBarStyleHorizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginRight="10.0dip"
                    android:layout_marginTop="10.0dip"
                    android:layout_toRightOf="@id/text_lefteye_shuzhi"
                    android:max="1500"
                    android:progressDrawable="@drawable/progressbaryz" />
            </RelativeLayout>

            <TextView
                android:id="@+id/text_lefteye_state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15.0dip"
                android:layout_marginTop="8.0dip"
                android:textColor="@color/black"
                android:textSize="14.0sp" />

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.0dip"
                android:layout_marginRight="10.0dip"
                android:layout_marginTop="30.0dip"
                android:background="@drawable/semangceshi" >

                <TextView
                    android:id="@+id/text_righteye_shuzhi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginTop="5.0dip"
                    android:textColor="@color/white"
                    android:textSize="16.0sp" />

                <ProgressBar
                    android:id="@+id/progress_right1"
                    style="?android:progressBarStyleHorizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginRight="10.0dip"
                    android:layout_marginTop="10.0dip"
                    android:layout_toRightOf="@id/text_righteye_shuzhi"
                    android:max="1500"
                    android:progressDrawable="@drawable/progressbar" />

                <ProgressBar
                    android:id="@+id/progress_right2"
                    style="?android:progressBarStyleHorizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginRight="10.0dip"
                    android:layout_marginTop="10.0dip"
                    android:layout_toRightOf="@id/text_righteye_shuzhi"
                    android:max="1500"
                    android:progressDrawable="@drawable/progressbaryb" />

                <ProgressBar
                    android:id="@+id/progress_right3"
                    style="?android:progressBarStyleHorizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginRight="10.0dip"
                    android:layout_marginTop="10.0dip"
                    android:layout_toRightOf="@id/text_righteye_shuzhi"
                    android:max="1500"
                    android:progressDrawable="@drawable/progressbaryz" />
            </RelativeLayout>

            <TextView
                android:id="@+id/text_righteye_state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15.0dip"
                android:layout_marginTop="8.0dip"
                android:textColor="@color/black"
                android:textSize="14.0sp" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="35.0dip"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/button_gotoceshi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginLeft="10.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/btn_button"
                    android:text="@string/retest"
                    android:textColor="@color/white"
                    android:textSize="25.0sp" />

                <Button
                    android:id="@+id/button_gotoback"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginRight="10.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/btn_button"
                    android:text="@string/back"
                    android:textColor="@color/white"
                    android:textSize="25.0sp" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>